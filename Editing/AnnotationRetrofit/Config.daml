<ArcGIS defaultAssembly="AnnotationRetrofit.dll" defaultNamespace="AnnotationRetrofit" xmlns="http://schemas.esri.com/DADF/Registry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.esri.com/DADF/Registry file:///C:/Program%20Files/ArcGIS/Pro/bin/ArcGIS.Desktop.Framework.xsd">
  <AddInInfo id="{3692f4db-e5e3-4ad6-9ac7-4b4ab629edc4}" version="1.0" desktopVersion="3.4.0" product="ArcGISPro">
    <Name>AnnotationRetrofit</Name>
    <Description>AnnotationRetrofit description</Description>
    <Image>Images\AddinDesktop32.png</Image>
    <Author>ArcGIS Pro SDK Team, arcgisprosdk@esri.com</Author>
