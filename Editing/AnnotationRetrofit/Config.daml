                    Content, Framework, Editing, Geodatabase, Geometry, Geoprocessing, Layouts, Map Authoring, Map Exploration -->
  </AddInInfo>
  <modules>
    <insertModule id="AnnotationRetrofit_Module" className="Module1" autoLoad="false" caption="Module1">
      <!-- uncomment to have the control hosted on a separate tab-->
      <tabs>
        <!--<tab id="AnnotationRetrofit_Tab1" caption="New Tab">
          <group refID="AnnotationRetrofit_Group1"/>
        </tab>-->
      </tabs>
      <groups>
        <!-- comment this out if you have no controls on the Addin tab to avoid
              an empty group-->
        <group id="AnnotationRetrofit_Group1" caption="Use Create Feature" keytip="G0" appearsOnAddInTab="true">
          <!--<tool refID="AnnotationRetrofit_SelectToRetrofit" size="large" />-->
          <button refID="AnnotationRetrofit_AnnotationRetrofit" size="large" />
        </group>
      </groups>
      <controls>
        <!-- add your controls here -->
        <tool id="AnnotationRetrofit_SelectToRetrofit" caption="Select To Retrofit" 
							keytip="T0" categoryRefID="esri_editing_construction_annotation" 
							className="SelectToRetrofit" loadOnClick="true" 
							smallImage="EditingAnnoEditTool16"
							largeImage="EditingAnnoEditTool32" condition="esri_mapping_mapPane">
          <tooltip heading="Retrofit Annotation for points">Select point features for which to create annotation<disabledText /></tooltip>
          <content guid="e2096d13-b437-4bc1-94ea-4494c3260f72" />
        </tool>
        <button id="AnnotationRetrofit_AnnotationRetrofit" caption="Annotation Retrofit" className="AnnotationRetrofit"
								loadOnClick="true" keytip="B1"
								smallImage="EditingAnnoEditTool16"
							  largeImage="EditingAnnoEditTool32">
          <tooltip heading="Annotation Retrofit">
						Annotation Retrofit button configures the Annotation Retrofit tool to be run through a button click<disabledText /></tooltip>
        </button>
      </controls>
    </insertModule>
  </modules>
