  <modules>
    <insertModule id="ConstructionSplitTool_Module" className="Module1" autoLoad="false" caption="Module1">     
      <controls>
        <!-- add your controls here -->
        <tool id="ConstructionSplitTool_LineSplitsPolygons" categoryRefID="esri_editing_construction_polyline" caption="LineSplitsPolygons" className="LineSplitsPolygons" loadOnClick="true" smallImage="GenericButtonRed16" largeImage="GenericButtonRed32" keytip="LSP"> 
          <!--note: use esri_editing_construction_polyline,  esri_editing_construction_polygon for categoryRefID as needed-->
          <tooltip heading="Split Polygon">Creates a line feature with the active template and splits intersecting polygon features in the map. The line is also split at points of intersection with the polygon feature(s).<disabledText /></tooltip>
          <content guid="5debd642-60ad-463d-adff-4a753c294e64" />
        </tool>
        <tool id="ConstructionSplitTool_PointSplitsLines" categoryRefID="esri_editing_construction_point" caption="PointSplitsLines" className="PointSplitsLines" loadOnClick="true" smallImage="GenericButtonRed16" largeImage="GenericButtonRed32" keytip="PSL">
          <!--note: use esri_editing_construction_polyline,  esri_editing_construction_polygon for categoryRefID as needed-->
          <tooltip heading="Split Line">Creates a point feature with the active template and splits any intersecting line features.<disabledText /></tooltip> 
          <content guid="a68a892e-6844-4f69-a1cd-3000ed1e0bc2" />
        </tool>
        <tool id="ConstructionSplitTool_LineSplitsLines" categoryRefID="esri_editing_construction_polyline" caption="LineSplitsLines" className="LineSplitsLines" loadOnClick="true" smallImage="GenericButtonRed16" largeImage="GenericButtonRed32" keytip="LSL">
          <!--note: use esri_editing_construction_polyline,  esri_editing_construction_polygon for categoryRefID as needed-->
          <tooltip heading="Split Line">Creates a line feature with the active template and splits intersecting lines from the same layer as well as other editable line layers in the map.<disabledText /></tooltip>
          <content guid="729b4a72-f0ae-4aac-98c8-1d5d1ee96aa0" />
        </tool>
      </controls>
