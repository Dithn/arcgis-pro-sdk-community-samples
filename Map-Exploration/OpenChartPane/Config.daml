  <modules>
    <insertModule id="OpenChartPane_Module" className="Module1" autoLoad="false" caption="Module1">
      <!-- uncomment to have the control hosted on a separate tab-->
      <tabs>
        <!--<tab id="OpenChartPane_Tab1" caption="New Tab">
          <group refID="OpenChartPane_Group1"/>
        </tab>-->
      </tabs>
      <groups>
        <!-- comment this out if you have no controls on the Addin tab to avoid
              an empty group-->
        <group id="OpenChartPane_Group1" caption="Group 1" appearsOnAddInTab="true">
          <!-- host controls within groups -->
          <button refID="OpenChartPane_OpenChartInTOC" size="large" />
          <button refID="OpenChartPane_CreateLayout" size="large" />
          <button refID="OpenChartPane_GenerateLayout" size="large" />
        </group>
      </groups>
      <controls>
        <!-- add your controls here -->
        <button id="OpenChartPane_OpenChartInTOC" caption="OpenChartInTOC" className="OpenChartInTOC" loadOnClick="true" smallImage="GenericButtonBlue16" largeImage="GenericButtonBlue32">
          <tooltip heading="Tooltip Heading">Tooltip text<disabledText /></tooltip>
        </button>
        <button id="OpenChartPane_CreateLayout" caption="CreateLayout" className="CreateLayout" loadOnClick="true" smallImage="GenericButtonBlue16" largeImage="GenericButtonBlue32">
          <tooltip heading="Tooltip Heading">Tooltip text<disabledText /></tooltip>
        </button>
        <button id="OpenChartPane_GenerateLayout" caption="GenerateLayout" className="GenerateLayout" loadOnClick="true" smallImage="GenericButtonBlue16" largeImage="GenericButtonBlue32">
          <tooltip heading="Tooltip Heading">Tooltip text<disabledText /></tooltip>
        </button>
      </controls>
    </insertModule>
  </modules>
